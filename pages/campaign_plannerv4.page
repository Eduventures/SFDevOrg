<apex:page standardController="Scenario__c" extensions="campaignPlannerControllerv4" sidebar="false" showHeader="true">
<apex:composition template="{!$Site.Template}">
<apex:define name="body">

<apex:includeScript id="jqbase" value="{!URLFOR($Resource.jqueryui, '/js/jquery-1.7.1.min.js')}"/>
<apex:includeScript id="jquijs" value="{!URLFOR($Resource.jqueryui, '/js/jquery-ui-1.8.18.custom.min.js')}"/>
<apex:stylesheet id="jquicss" value="{!URLFOR($Resource.jqueryui, '/css/smoothness/jquery-ui-1.8.18.custom.css')}"/> 
<style>
.currentStep { 
		font-weight:bold;
		color: blue;
}

.bigHeader { white-space: no-wrap !important; 
             background-color: #C5D9F1 !important;
             width: 75%;}
.colHeader { white-space: pre-wrap !important; 
             background-color: #C5D9F1 !important;
             width: 25%;
             text-align:right;}             
.myInput { width:35px !important; }
table.cattable {
    width: 400px;
    border-width: 1px;
    border-spacing: 0px;
    border-style: solid;
    border-color: black;
    border-collapse: collapse;
}

table.cattable td {
    border-width: 1px;
    padding: 5px;
    border-style: inset;
    border-color: black;
}

table.cattable td.blank {
    border: none !important;
	background-color: #FFFFFF !important;
}
</style>
<script>
    function confirmation(type) 
    {
        return confirm('Are you sure you want to delete this ' + type + ' ? It cant be undone.');
    }
        
    function showMe(divID) 
    {
            $("[id='"+divID+"']").dialog('open');    
    }
    
    function hideMe(divID) 
    {
        $("[id='"+divID+"']").dialog('close');    
    }
    
    $(document).ready( function(){
    
        $(".popupWindow").dialog(
        {
            modal:true,
            autoOpen: false,
            closeable: true,
            resizable: true,
            width: 'auto',
            height: 'auto',
            overlay: {opacity: 0.5, background: "black" },
            open: function(event, ui) { $(".ui-dialog-titlebar-close").remove(); }      
        });
        
    });
    
</script>
<apex:form > 
     <apex:pageBlock title="Overview - Campaign Name:{!IF(theScenario.Campaign_Name__c != null, theScenario.Campaign_Name__c,'')}" id="overview">
        <apex:pageBlockButtons location="top" >
            <apex:commandButton action="{!backTomain}" value="Go Back To Main Page" />
        </apex:pageBlockButtons>
        <p>The goal of the Campaign Staffing Tool is to demonstrate an institution's visit capacity for smaller focused
        campaigns in order to help institutions make the case for more resources. The tool incorporates current staffing levels and assumptions
         in regards to the number of visits required for closing gifts at differing levels within the gift pyramid.</p>

        <p>Steps to complete: {!theScenario.Last_Good_Step__c}</p>
		<ol id="steplist"> 
			<li><apex:outputText rendered="{!stepCount==1}">Enter a name for the Campaign</apex:outputText>
				<apex:commandLink action="{!jumpto}" value="Enter a name for the Campaign" rendered="{!stepCount!=1}"><apex:param value="{!1}" assignTo="{!jumpto}" /> </apex:commandLink>
			</li>
			<li><apex:outputText rendered="{!stepCount==2 || theScenario.Last_Good_Step__c ==1}">Define the tiers of the Gift Pyramid</apex:outputText>
				<apex:commandLink action="{!jumpto}" value="Define the tiers of the Gift Pyramid" rendered="{!theScenario.Last_Good_Step__c > 1 && stepCount!=2}"><apex:param value="{!2}" assignTo="{!jumpto}" /> </apex:commandLink>
			</li>
			<li><apex:outputText rendered="{!stepCount==3 || theScenario.Last_Good_Step__c<=2}">Identify any staff positions that will be involved in the frontline effort</apex:outputText>
				<apex:commandLink action="{!jumpto}" value="Identify any staff positions that will be involved in the frontline effort" rendered="{!theScenario.Last_Good_Step__c > 2 && stepCount!=3}"><apex:param value="{!3}" assignTo="{!jumpto}" /> </apex:commandLink>
			</li>
			<li><apex:outputText rendered="{!stepCount==4 || theScenario.Last_Good_Step__c<=3}">Break down each Gift Tier into more discrete gift levels</apex:outputText>
				<apex:commandLink action="{!jumpto}" value="Break down each Gift Tier into more discrete gift levels" rendered="{!theScenario.Last_Good_Step__c > 3 && stepCount!=4}"><apex:param value="{!4}" assignTo="{!jumpto}" /> </apex:commandLink>
			</li>
			<li><apex:outputText rendered="{!stepCount==5 || theScenario.Last_Good_Step__c<=4}">Review the Campaign Model capacity results</apex:outputText>
				<apex:commandLink action="{!jumpto}" value="Review the Campaign Model capacity results" rendered="{!theScenario.Last_Good_Step__c > 4 && stepCount!=5}"><apex:param value="{!5}" assignTo="{!jumpto}" /> </apex:commandLink>
			</li>
			<li><apex:outputText rendered="{!stepCount==6 || theScenario.Last_Good_Step__c<=5}">Update Staffing Assumptions &amp; Actuals (# of FTEs, visit capacity, actual visits)</apex:outputText>
				<apex:commandLink action="{!jumpto}" value="Update Staffing Assumptions & Actuals (# of FTEs, visit capacity, actual visits)" rendered="{!theScenario.Last_Good_Step__c > 5 &&stepCount!=6}"><apex:param value="{!6}" assignTo="{!jumpto}" /> </apex:commandLink>
			</li>
		</ol>
	<script>
    	$('#steplist li:nth-child(<apex:outputText value="{!stepCount}" />)').addClass('currentStep');
	</script>
	</apex:pageBlock>
    <div style="text-align: center;width:100%;">&nbsp;
    	<apex:actionStatus id="editsaveStatus" >
	    <apex:facet name="start">
	     <h1>(loading ...) <img src="{!$Resource.spin}"/>  </h1>
	    </apex:facet>
	    </apex:actionStatus>
	    <apex:actionStatus id="cateditsaveStatus" >
	    <apex:facet name="start">
	     <h1>(Saving edit &amp; recalculating...) <img src="{!$Resource.spin}"/>  </h1>
	    </apex:facet>
	    </apex:actionStatus>
    </div>
    <apex:pageMessages id="msgs" ></apex:pageMessages>		
	<apex:outputPanel id="campaignName" >
	<apex:pageBlock title="Step 1: Name the Campaign" rendered="{!stepCount == 1}">
        <apex:actionRegion >
        <p> To get started, please enter a name that describes the campaign you want to model: 
            <apex:inputField value="{!theScenario.Campaign_Name__c}" style="width:400px;"/> and 
            then hit &nbsp;<apex:commandButton value="Continue" action="{!next}" status="editsaveStatus"  reRender="giftpyramid,campaignName,overview" />    
        </p> 
        </apex:actionRegion>
    </apex:pageBlock>     
	</apex:outputPanel>  

    <apex:outputPanel id="giftpyramid" >
    <apex:pageBlock title="STEP 2: Specify the Tiers of the 'Gift Pyramid'" rendered="{! stepCount == 2}">
		<script>
	    	$('#steplist li').removeClass('currentStep');
	    	$('#steplist li:nth-child(<apex:outputText value="{!stepCount}" />)').addClass('currentStep');
		</script>
            <p>Tiers are used to designate an upper and lower limit on the size of gifts that staff in various positions will solicit. You 
            will have the ability to list the staff groups, and which Tier they will target, in later steps. Identify the upper and lower 
            thresholds of the tier, and leave blank if there is no limit (say, for the Upper threshold on your top tier).<br /></p>
            <p>When you are ready, select &nbsp;<apex:commandButton value="Next" action="{!next}" reRender="positions,giftpyramid,msgs,overview" status="editsaveStatus" />
        	 or click <apex:commandButton value="Back" action="{!back}" reRender="giftlevels,giftpyramid,overview"  status="editsaveStatus" /> to edit the Campaign Name</p> 
            
            
            <table>
            <tr>
            	<td>To get started, <apex:commandButton value="click to make a new Gift Tier" action="{!newTier}" reRender="giftpyramid,msgs" status="editsaveStatus" />
            	</td>
            	<td style="vertical-align:top;">
	            <apex:outputPanel id="categoriesBox" >
	                <apex:actionRegion >
	                <p><b>List of Gift Tiers</b></p>
	                <table>
                	<tr>
                		<td><b>Min Gift Value</b></td>
                		<td><b>Max Gift Value (leave blank if no cap)</b></td>
                		<td><b>Actions</b></td>
                	</tr>
                	<apex:outputPanel layout="none" id="addTierBox" rendered="{! makingnewtier }" >
		            <apex:actionRegion >
	                <tr>               
	                	<td>$<apex:inputField value="{!addCategory.Threshold_Lower__c}"/></td>
	                	<td>$<apex:inputField value="{!addCategory.Threshold_Upper__c}"/></td>
	                	<td>
	                		<apex:commandButton action="{!saveCedit}" reRender="addTierBox,categoriesBox,positions" value="Save" status="editsaveStatus" />
	                    	<apex:commandButton action="{!cancelCedit}" reRender="addTierBox,categoriesBox" value="Cancel" status="editsaveStatus" />
	                    </td>
	                </tr>
		            </apex:actionRegion>    
		            </apex:outputPanel>
	                <apex:repeat value="{!categories}" var="category">
	                <tr style="background-color:{!category.Color__c} !important;">
	                	<td>
	                		<apex:outputField value="{!category.Threshold_Lower__c}" rendered="{!editCategoryId != category.Id}"/>
	                		<apex:inputField value="{!category.Threshold_Lower__c}" rendered="{!editCategoryId == category.Id}"/>
                		</td>
	                	<td>
	                		<apex:outputField value="{!category.Threshold_Upper__c}" rendered="{!editCategoryId != category.Id}"/>
	                		<apex:inputField value="{!category.Threshold_Upper__c}" rendered="{!editCategoryId == category.Id}"/>
	                	</td>
	                    <td>
		                    <apex:commandButton value="Edit" action="{!editCategory}" rendered="{!editCategoryId == null}" reRender="categoriesBox" status="editsaveStatus" >
		                        <apex:param name="editCategoryId"
		                        value="{!category.Id}"
		                        assignTo="{!editCategoryId}"/> 
		                    </apex:commandButton>
		                    <apex:commandButton value="Delete" action="{!deleteCategory}" rendered="{!editCategoryId == null}" reRender="categoriesBox,positions" status="editsaveStatus" >
		                        <apex:param name="deleteCategoryId"
		                        value="{!category.Id}"
		                        assignTo="{!deleteCategoryId}"/>
		                    </apex:commandButton>
		                    <apex:commandButton action="{!saveCedit}" reRender="addTierBox,categoriesBox,positions" value="Save" rendered="{!editCategoryId != null}" status="editsaveStatus" />
		                    <apex:commandButton action="{!cancelCedit}" reRender="addTierBox,categoriesBox" value="Cancel" rendered="{!editCategoryId != null}" status="editsaveStatus" />
						</td>
					</tr>
	                </apex:repeat>
					</table>
	                </apex:actionRegion>       
	            </apex:outputPanel>
            	</td>
            </tr>
            </table>
        </apex:pageBlock>
		</apex:outputPanel>  
		
        <apex:outputPanel id="positions" >
        <apex:pageBlock title="STEP 3: identify your Frontline Staffing Groups" rendered="{!stepCount == 3}">
		<script>
	    	$('#steplist li').removeClass('currentStep');
	    	$('#steplist li:nth-child(<apex:outputText value="{!stepCount}" />)').addClass('currentStep');
		</script>
            <apex:outputPanel id="editBox" >
                <p>Identify the staff groups that will be fundraising for this particular campaign, noting the percentage of their time that will be 
                dedicated to frontline work. Select 'add a position' next to the gift tier that those staff will be fundraising for.</p>
    			<p>When you are ready, select &nbsp;<apex:commandButton value="Next" action="{!next}" reRender="positions,giftlevels,msgs,overview" status="editsaveStatus" />
        	 	or click <apex:commandButton value="Back" action="{!back}" reRender="positions,giftpyramid,overview" status="editsaveStatus" /> to edit your Gift Tiers</p> 
                
                <table style="width:100%">
	            <tr>
	                <td style="vertical-align:top;width:50%">
	                <apex:outputPanel id="categoriesPOBox" >
	                <apex:actionRegion >
	                <p><b>List of Gift Tiers</b></p>
	                <apex:pageBlockTable value="{!categories}" var="category">
	                <apex:column style="white-space:nowrap;font-weight:bold;" >
	                    <apex:facet name="header">Tier Name</apex:facet>
	                    <apex:outputField value="{!category.Name}"/>            
	                </apex:column>
	                <apex:column >
	                    <apex:facet name="header"></apex:facet>
	                    <apex:commandButton value="Add a position at this level" action="{!addPosition}" disabled="{! levelCategoryId != null || editPositionId != null }" reRender="positions,addPO" status="editsaveStatus" >
	                        <apex:param name="levelCategoryId"
	                        value="{!category.Id}"
	                        assignTo="{!levelCategoryId}"/>
	                    </apex:commandButton> 
	                </apex:column>
	                <apex:column >
	                    <apex:facet name="header">Min Gift Value</apex:facet>
	                    <apex:outputField value="{!category.Threshold_Lower__c}"/>
	                </apex:column> 
	                <apex:column >
	                    <apex:facet name="header">Max Gift Value</apex:facet>
	                    <apex:outputField value="{!category.Threshold_Upper__c}"/>
	                </apex:column>
					<apex:column width="50%" >
						<apex:facet name="header">Staff Groups Fundraising at this Tier</apex:facet>
						<apex:outputPanel id="positionsBox" >
							<table>
			                <apex:outputPanel id="positionsheader" layout="none" rendered="{!levelCategoryId == category.Id || category.Staffing_Positions__r.size > 0}" >
			                <tr>
			                	<td style="white-space: nowrap;font-weight:bold;" >Position Name</td>
			                	<td style="white-space: nowrap;font-weight:bold;" >Actions</td>
			                	<td style="white-space: nowrap;font-weight:bold;" >% of time dedicated to campaign</td>
			                	<td style="white-space: nowrap;font-weight:bold;" >Expected # of visits (per year)</td>
			                	<td style="white-space: nowrap;font-weight:bold;" ># of FTEs in this position</td>
			                	<td style="white-space: nowrap;font-weight:bold;" >Gift Tier</td>
			                </tr>
			                </apex:outputPanel>
			                <apex:actionRegion >							
			                <apex:outputPanel id="addPO" layout="none" rendered="{!levelCategoryId == category.Id && editPositionId == null }">
			                <tr>
			                	<td>
								<apex:inputField value="{!addPosition.Name}"/>
								</td>			                	
			                	<td>
			                		<apex:commandButton action="{!savePedit}" reRender="positions,editBox,msgs" value="Save" status="editsaveStatus" />
				                	<apex:commandButton action="{!cancelPedit}" reRender="positions" value="Cancel" status="editsaveStatus" />
			                	</td>            
			                	<td><apex:inputField value="{!addPosition.Percent_of_Time__c}"/></td>	
			                	<td><apex:inputField value="{!addPosition.Expected_Visits_per_Year__c}"/></td>
			                	<td><apex:inputField value="{!addPosition.Number_of_FTE__c}"/></td>
			                	<td> </td>
			                </tr>
				            </apex:outputPanel>
				            </apex:actionRegion>
			                <apex:repeat value="{!category.Staffing_Positions__r}" var="position">		                
			                <tr>
			                	<td>
			                		<apex:outputField value="{!position.Name}" rendered="{! editPositionId != position.Id}" />
			                    	<apex:inputField value="{!position.Name}" rendered="{! editPositionId == position.Id}" />
			                    </td>
			                    <td>
				                    <apex:commandButton value="Edit" action="{!editPosition}" rendered="{! levelCategoryId == null && editPositionId == null }" reRender="editBox,positionsBox" status="editsaveStatus" >
				                        <apex:param name="editPositionId"
				                        value="{!position.Id}"
				                        assignTo="{!editPositionId }"/>
				                        <apex:param name="levelCategoryId"
				                        value="{!category.Id}"
				                        assignTo="{!levelCategoryId}"/>
				                    </apex:commandButton>
				                    <apex:commandButton value="Delete" action="{!deletePosition}" rendered="{! levelCategoryId == null && editPositionId == null }" reRender="editBox,positionsBox" status="editsaveStatus" >
				                        <apex:param name="deletePositionId"
				                        value="{!position.Id}"
				                        assignTo="{!deletePositionId}"/>
				                        <apex:param name="levelCategoryId"
				                        value="{!category.Id}"
				                        assignTo="{!levelCategoryId}"/>			                        
				                    </apex:commandButton>
				                    <apex:commandButton action="{!savePedit}" reRender="positions,editBox,msgs" value="Save" rendered="{! editPositionId == position.Id}" status="editsaveStatus" />
					                <apex:commandButton action="{!cancelPedit}" reRender="positions,msgs" value="Cancel" rendered="{! editPositionId == position.Id}" status="editsaveStatus" />
			                    </td>
				                <td> 
				                    <apex:outputField value="{!position.Percent_of_Time__c}" rendered="{! editPositionId != position.Id}" />
				                    <apex:inputField value="{!position.Percent_of_Time__c}" rendered="{! editPositionId == position.Id}" />
								</td>
				                <td> 
				                    <apex:outputField value="{!position.Expected_Visits_per_Year__c}" rendered="{! editPositionId != position.Id}" />
				                    <apex:inputField value="{!position.Expected_Visits_per_Year__c}" rendered="{! editPositionId == position.Id}" />
								</td>  
				                <td> 
				                    <apex:outputField value="{!position.Number_of_FTE__c}" rendered="{! editPositionId != position.Id}" />
				                    <apex:inputField value="{!position.Number_of_FTE__c}" rendered="{! editPositionId == position.Id}" />
								</td>
				                <td> 
				                    <apex:outputText value="{!position.Staffing_Category__r.Name}"  />
				                    <apex:outputPanel rendered="{! editPositionId == position.Id}"  >
				                    	<a href="javascript:tierHelp()">want to change?</a>
					                    <script language="JavaScript" type="text/javascript">
						                    function tierHelp(){
							                    alert("This can't be changed, please delete the position and recreate it if you need to change it")
											}
					                    </script>
				                    </apex:outputPanel>
								</td>  			                    
			                </tr>                          
		                	</apex:repeat>
		                	</table>       
		            	</apex:outputPanel>
					</apex:column>	                
	                </apex:pageBlockTable>
	                </apex:actionRegion>       
	            	</apex:outputPanel>
	            	</td>
	            </tr>
	            </table>
   
            </apex:outputPanel>
        </apex:pageBlock>                       
        </apex:outputPanel>

          
        <apex:outputPanel id="giftlevels">     
        <apex:pageBlock title="STEP 4: Identify the Gift Levels within each Tier" rendered="{! stepCount == 4}">
		<script>
	    	$('#steplist li').removeClass('currentStep');
	    	$('#steplist li:nth-child(<apex:outputText value="{!stepCount}" />)').addClass('currentStep');
		</script>
	    <apex:outputPanel id="scenariodata" title="Scenario Data" style="width:100%;" >
	    Define gift levels within each Tier of the pyramid (these levels help identify the expected # of donors, 
	    prospects and visits you think will	be needed for soliciting donations within a specific dollar range)
	    <p>When you are ready, select &nbsp;<apex:commandButton value="Next" action="{!next}" reRender="giftlevels,campaignmodel,msgs,overview" status="editsaveStatus" />
        	 or <apex:commandButton value="Back" action="{!back}" reRender="giftlevels,positions,overview" status="editsaveStatus" /></p>
                <apex:repeat value="{!categories}" var="cat"> 
                <table class="cattable" style="width:100%;">          
	                <tr style="background-color:{!cat.Color__c} !important;">
	                	<td colspan="8"><b><apex:outputField value="{!cat.Name}"  /></b>
						<apex:commandButton action="{!setNewLevelCatId}" value="Define a New Level in this tier" rerender="giftlevels,addlevelBox,gts" status="editsaveStatus" >
                    		<apex:param name="categoryid" value="{!cat.Id}" assignTo="{!levelCategoryId}" />
                    	</apex:commandButton>  
                    	</td>	
					</tr>      
	                <tr style="background-color:{!cat.Color__c} !important;" cellpadding="5px">
		                <td class="blank">Available Actions <br />
	                    </td>
		                <td>Minimum Gift Value</td>
		                <td>Maximum Gift Value</td>
		                <td>Expected Donation Total (in $)</td>
		                <td>Number of Prospects Needed</td>
		                <td>Number of Donors Required</td>               
		                <td>Expected Number of Visits Per Donor</td>
		                <td>Total Number of Donor Visits</td>
	                </tr>
					<apex:actionRegion >
			        <apex:outputPanel layout="none" id="addlevelBox" rendered="{!levelCategoryId == cat.Id && editLevelId == null}" >
			        <tr>		
			        	<td style="text-align:left;color:blue;"><b>Add a Gift Level</b><br /> 
			        		<apex:commandButton action="{!saveLedit}" reRender="addlevelBox,scenariodata,msgs,gts" value="Save" status="editsaveStatus" />
			            	<apex:commandButton action="{!cancelLedit}" reRender="addlevelBox,scenariodata,msgs,gts" value="Cancel" status="editsaveStatus" />
			            </td>
			            <td><apex:inputField value="{!addLevel.Minimum_Gift_Value__c}" /></td>
			            <td><apex:inputField value="{!addLevel.Maximum_Gift_Value__c}" /></td>
			            <td><apex:inputField id="totalgiftvalue" value="{!addLevel.Total_Gifts_Equalling__c}" /></td>                       
			            <td><apex:inputField id="numprospects" value="{!addLevel.Number_Of_Prospects__c}" /></td>
			            <td><apex:inputField value="{!addLevel.Number_Of_Donors__c}"/></td>
			            <td><apex:inputField id="numvisits" value="{!addLevel.Number_of_Visits_Per_Prospect__c}" /></td>
					</tr>    
					</apex:outputPanel>              
				    </apex:actionRegion>              				                                             
	                <apex:repeat value="{!cat.staffing_levels__r}" var="level">        
	                <tr style="background-color:{!cat.Color__c} !important;">
	                	<td class="blank">
						<apex:commandButton value="Edit" action="{!editLevel}" rendered="{!editLevelId == null && levelCategoryId == null}" reRender="editlevelBox,scenariodata" oncomplete="showMe('levelEditWindow');" status="editsaveStatus" >
	                        <apex:param name="editLevelId" value="{!level.Id}" assignTo="{!editLevelId}"/>
	                        <apex:param name="categoryid" value="{!cat.Id}" assignTo="{!levelCategoryId}" />
	                    </apex:commandButton>
	                    <apex:commandButton action="{!deleteLevel}" rendered="{!editLevelId == null && levelCategoryId == null}" value="Delete"  style="color:red;" onclick="if (!confirmation('level')) return false;" rerender="editlevelBox,scenariodata" status="editsaveStatus" >
	                    	<apex:param name="levelid" value="{!level.Id}" assignTo="{!deletelevelId}"  />
	                        <apex:param name="categoryid" value="{!cat.Id}" assignTo="{!levelCategoryId}" />
	                    </apex:commandButton>
	                    <apex:commandButton action="{!saveLedit}" rendered="{!level.Id == editLevelId}"  reRender="addlevelBox,scenariodata,msgs,gts" value="Save" status="editsaveStatus">
	                    	<apex:param name="editLevelId" value="{!level.Id}" assignTo="{!editLevelId}"/>
	                    	<apex:param name="categoryid" value="{!cat.Id}" assignTo="{!levelCategoryId}" />
	                    </apex:commandButton>
			            <apex:commandButton action="{!cancelLedit}" rendered="{!level.Id == editLevelId}" reRender="addlevelBox,scenariodata,msgs,gts" value="Cancel" status="editsaveStatus" />
	                    </td>
	                    <td>
	                    	<apex:outputField id="giftvalue" value="{!level.Minimum_Gift_Value__c}" rendered="{! editLevelId != level.Id}" />
	                    	<apex:inputField value="{!level.Minimum_Gift_Value__c}" rendered="{! editLevelId == level.Id}" />
	                    </td>
	                    <td>
	                    	<apex:outputField id="maxgiftvalue" value="{!level.Maximum_Gift_Value__c}" rendered="{! editLevelId != level.Id}" />
							<apex:inputField value="{!level.Maximum_Gift_Value__c}" rendered="{! editLevelId == level.Id}" />           	             	
	                    </td>	                    
	                    <td>
	                    	<apex:outputField id="totalgiftvalue" value="{!level.Total_Gifts_Equalling__c}" rendered="{! editLevelId != level.Id}" />
	                    	<apex:inputField value="{!level.Total_Gifts_Equalling__c}" rendered="{! editLevelId == level.Id}" />
	                    </td>
	                    <td>
	                    	<apex:outputField id="numprospects" value="{!level.Number_Of_Prospects__c}" rendered="{! editLevelId != level.Id}" />
	                    	<apex:inputField value="{!level.Number_Of_Prospects__c}" rendered="{! editLevelId == level.Id}" />
	                    </td>
	                    <td>
	                    	<apex:outputField value="{!level.Number_Of_Donors__c}" rendered="{! editLevelId != level.Id}" />
	                    	<apex:inputField value="{!level.Number_Of_Donors__c}" rendered="{! editLevelId == level.Id}" />
	                    </td>                                     
	                    <td>
	                    	<apex:outputField id="numvisits" value="{!level.Number_of_Visits_Per_Prospect__c}" rendered="{! editLevelId != level.Id}" />
	                    	<apex:inputField value="{!level.Number_of_Visits_Per_Prospect__c}" rendered="{! editLevelId == level.Id}" />
